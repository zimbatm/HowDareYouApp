{
  "version": "<%= my_version %>",
  "name": "HowDareYou",
  "description": "An app to scold your friends. With a French accent !",
  "icons": {
  <% %w[16 32 48 64 128].each do |size| %>
    "<%= size %>": "<%= asset_path :images, "icon-#{size}.png" %>"<%= ',' unless size == '128' %>
  <% end %>
  },
  "developer": {
    "name": "zimbatm",
    "url": "http://zimbatm.com"
  },
  "installs_allowed_from": ["*"],
  "appcache_path": "/how-dare-you.appcache",
  "orientation": "portrait",
  "fullscreen": "true"
}
